# Default DB
jdbc.driverClass=net.sf.log4jdbc.DriverSpy
jdbc.url=jdbc:hsqldb:hsql://127.0.0.1:6789/BDWebSampleDB
jdbc.username=sa
jdbc.password=

jdbc.initialSize=0
jdbc.initSQL=
jdbc.minIdle=0
jdbc.maxIdle=8
jdbc.maxActive=8
jdbc.maxWait=6000
jdbc.jmxEnabled=true
jdbc.jdbcInterceptors=
jdbc.removeAbandoned=false
jdbc.removeAbandonedTimeout=60
jdbc.logAbandoned=false
jdbc.testOnBorrow=true
jdbc.testOnReturn=false
jdbc.testWhileIdle=false
jdbc.useEquals=false
jdbc.fairQueue=false
jdbc.timeBetweenEvictionRunsMillis=30000
jdbc.minEvictableIdleTimeMillis=30000
jdbc.validationQueryTimeout=10000
jdbc.validationQuery=SELECT 1 FROM DUAL

