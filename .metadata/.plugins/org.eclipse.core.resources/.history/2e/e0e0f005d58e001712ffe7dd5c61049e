	/**
	 * RESTful API Module Error Code Base String
	 */	
	public static final String API_BASE		=	"EAPI";
	/**
	 * Batch Module Error Code Base String
	 */		
	public static final String BATCH_BASE	=	"EBAT";
	/**
	 * BusinessI Module Error Code Base String
	 */		
	public static final String BIZ_BASE		=	"EBIZ";
	/**
	 * Common Module Error Code Base String
	 */	
	public static final String COMMON_BASE	=	"ECMN";
	/**
	 * Front Module Error Code Base String
	 */	
	public static final String FRONT_BASE	=	"EFRO";
	/**
	 * Library Module Error Code Base String
	 */	
	public static final String LIBRARY_BASE	=	"ELIB";

