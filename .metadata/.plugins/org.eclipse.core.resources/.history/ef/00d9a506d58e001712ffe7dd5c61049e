public class BizModuleException extends ApplicationException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	/**
	 * Desc : Constructor of BizModuleException.java class
	 */

	public BizModuleException() {
		// TODO Auto-generated constructor stub
	}

	/**
	 * Desc : Constructor of BizModuleException.java class
	 * @param errorCode
	 */

	public BizModuleException(String errorCode) {
		super(ExceptionCodeBaseConstants.BIZ_BASE + errorCode);
		// TODO Auto-generated constructor stub
	}

	/**
	 * Desc : Constructor of BizModuleException.java class
	 * @param throwable
	 */

	public BizModuleException(Throwable throwable) {
		super(throwable);
		// TODO Auto-generated constructor stub
	}

	/**
	 * Desc : Constructor of BizModuleException.java class
	 * @param errorCode
	 * @param throwable
	 */

	public BizModuleException(String errorCode, Throwable throwable) {
		super(ExceptionCodeBaseConstants.BIZ_BASE + errorCode, throwable);
		// TODO Auto-generated constructor stub
	}

}
