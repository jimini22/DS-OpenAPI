<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mini.portal.board.mapper.BoardMapper">

	<select id="getBoardList" resultType="BoardVO">
		SELECT
			brd_id,
			brd_title,
			brd_description,
			brd_order,
			created_by,
			created_date,
			modified_by,
			modified_date
		FROM board
	</select>

</mapper>